# PostgreSQL Configuration
POSTGRES_USER=eoir
POSTGRES_PASSWORD=changeme
POSTGRES_DB=eoir
POSTGRES_HOST=postgres  # Use 'postgres' for Docker, 'localhost' for local development
POSTGRES_PORT=5434      # External port on host machine (change if 5434 is also in use)

# Note: Inside Docker, PostgreSQL always runs on port 5432
# The POSTGRES_PORT above is only for mapping the external port

# Logging
LOG_LEVEL=INFO