# SocialMapper Environment Configuration
# Copy this file to .env and fill in your values

# Census Bureau API Key (optional but recommended)
# Get your free API key at: https://api.census.gov/data/key_signup.html
CENSUS_API_KEY=your_census_api_key_here

# Census API Configuration (optional - defaults provided)
CENSUS_API_BASE_URL=https://api.census.gov/data
CENSUS_API_TIMEOUT=30

# Rate Limiting Configuration (optional)
CENSUS_RATE_LIMIT=60
CENSUS_RATE_BURST=10

# Caching Configuration (optional)
CENSUS_CACHE_ENABLED=true
CENSUS_CACHE_TTL=3600
CENSUS_CACHE_MAX_SIZE=1000

# Retry Configuration (optional)
CENSUS_MAX_RETRIES=3
CENSUS_RETRY_BACKOFF=0.5

# Logging Configuration (optional)
CENSUS_LOG_LEVEL=INFO
CENSUS_LOG_API=false

# Data Storage Configuration (optional)
CENSUS_ENABLE_REPOSITORY=false
CENSUS_REPOSITORY_PATH=

# Other API Keys (if needed for future features)
# OPENSTREETMAP_API_KEY=
# GOOGLE_MAPS_API_KEY= 