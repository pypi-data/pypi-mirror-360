{
   "ietf-restconf-server:restconf-server": {
       "listen": {
           "endpoints": {
               "endpoint": [
                   {
                       "name": "default startup endpoint",
                       "yangcore:use-for": "native-interface",
                       "http-over-tcp": {
                           "tcp-server-parameters": {
                               "local-bind": [
                                   {
                                       "local-address": "127.0.0.1",
                                       "local-port": 8080
                                   },
                                   {
                                       "local-address": "::1",
                                       "local-port": 8080
                                   }
                               ]
                           }
                       }
                   }
               ]
           }
       }
   },
   "yangcore:preferences": {
       "authentication": {
           "internal-authenticator": {
               "passwords-allowed": {}
           }
       }
   }
}

