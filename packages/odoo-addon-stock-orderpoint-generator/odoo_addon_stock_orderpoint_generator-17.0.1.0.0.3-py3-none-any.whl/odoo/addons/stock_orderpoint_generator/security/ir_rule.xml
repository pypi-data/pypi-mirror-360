<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="1">
    <record id="warehouse_comp_rule" model="ir.rule">
        <field name="name">Restriction by company in warehouses</field>
        <field name="model_id" ref="model_stock_warehouse_orderpoint_template" />
        <field name="global" eval="True" />
        <field
            name="domain_force"
        >[('company_id', 'in', user.company_ids.ids + [False])]</field>
    </record>
</odoo>
