        "type": {
          "default": "color",
          "title": "Type",
          "type": "string"
        },
        "publish_camera_info": {
          "default": true,
          "title": "Publish Camera Info",
          "type": "boolean"
        },
        "ptz": {
          "default": false,
          "title": "Ptz",
          "type": "boolean"
        },
        "camera_offsets": {
          "anyOf": [
