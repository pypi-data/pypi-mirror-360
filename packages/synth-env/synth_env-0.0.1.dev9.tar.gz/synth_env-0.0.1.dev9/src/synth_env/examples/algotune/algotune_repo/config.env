# config.env - Unified AlgoBench Configuration
# This file contains all configuration settings for AlgoBench timing evaluations

# ===============================================================================
# COMPUTE ENVIRONMENT
# ===============================================================================

# Singularity container image path
export SINGULARITY_IMAGE="/path/to/image.sif"

# Data storage directory (where datasets and results are stored)
export DATA_DIR="/path/to/data/dir"

# Temporary directory for intermediate files
export TEMP_DIR_STORAGE="/path/to/temp/dir"

# ===============================================================================
# SLURM SETTINGS
# ===============================================================================

# Default SLURM partition for compute jobs
export SLURM_PARTITIONS_DEFAULT="default_partition"

# SLURM partition for agent-based jobs
export SLURM_PARTITIONS_AGENT="agent_partition"
