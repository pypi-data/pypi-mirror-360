# A sample .pitlakq file.
# Rename to .pitlakq and put in your home directory.
# If an environmental varibale PITLAKQHOME exists it will used for to
# look for .pitlakq.

model_path:
    c:\my\path\to\pitlakq_work\models
     
ram_path:
    c:\my\path\to\pitlakq_work\RAM

resources_path:
    c:\my\path\to\pitlakq_work\pitlakq_exe\pitlakq\resources

template_path:
    c:\my\path\to\pitlakq_work\pitlakq_exe\pitlakq\templates